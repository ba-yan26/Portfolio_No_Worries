<div class="admin-container">
  <h4 class="main-title">通報一覧</h4>

  <table class ="table">
    <thead>
      <tr>
        <th></th>
        <th>タイトル</th>
        <th>該当者</th>
      </tr>
    </thead>
    <tbody>
      <% @notices.each do |notice| %>
        <tr>
          <td class="admin-notice-time"><%= notice.created_at.strftime("%Y/%m/%d %H:%M") %></td>
          <td>
            <%= link_to admin_notice_path(notice) do %>
              <%= notice.title %>
            <% end %>
          </td>
          <td><%= link_to notice.end_user.name, edit_admin_end_user_path(notice.end_user) %></td>
       </tr>
      <% end %>
    </tbody>
  </table>
</div>