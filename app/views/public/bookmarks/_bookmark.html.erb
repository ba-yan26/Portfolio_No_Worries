<% if room.is_bookmarked_by?(current_end_user) %>
  <%= link_to room_bookmarks_path(room), method: :delete, remote: true do %>
    <i class="fas fa-star delete"></i>
  <% end %>
<% else %>
  <%= link_to room_bookmarks_path(room), method: :post, remote: true do %>
    <i class="fas fa-star post"></i>
  <% end %>
<% end %>
